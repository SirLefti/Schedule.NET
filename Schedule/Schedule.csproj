        <PackOnBuild>true</PackOnBuild>
        <PackageId>Schedule.NET</PackageId>
        <PackageVersion>1.7</PackageVersion>
        <Authors>SirLefti</Authors>
        <Copyright>Apache 2.0</Copyright>
